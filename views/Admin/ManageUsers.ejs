                <div class="row">
                      
      
                    <div class="col-lg-10">
                        <h2>Manage Users</h2>
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th width="50px">No</th>
                                         <th width="50px">User Id</th>
                                            <th>User Name</th>
                                            <th>Password</th>
                                            <th>Role</th>
                                            <th width="120px">Action</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                        <% if(usersData.length>0){ 
                                
                                            for(var i = 0;i < usersData.length;i++) { %>
                 
                                       
                                    <tr>
                                         
                                       <td><%=(i+1)%></td>
                                        <td><%=usersData[i].adminuserid%></td>
                                            <td><%=usersData[i].uname%></td>
                                            <td><%=usersData[i].password%></td>
                                            <td><%=usersData[i].role%></td>
                                             <td>
                                        <a class="a-inside edit" style="cursor:pointer;" 
                                            href="/admin/Manage/<%=usersData[i].adminuserid%>">Edit</a>
                       
                                        <a class="a-inside delete" data-toggle="modal" data-target="#myModalDiscard" >Delete</a>
                       
                                            </td>
                                    </tr>
                                      <div class="container">



                                    <div class="modal fade" id="myModalDiscard" role="dialog">
                                <div class="modal-dialog">

                                        <!-- Modal content-->
                                        <div class="modal-content">

                                        <div class="modal-body">
                                            <p>Are you sure you want to delete user ?</p>
                                              <button class="btn saved" id="" onClick="discard(<%=usersData[i].adminuserid%>)" data-dismiss="modal">YES</button>
                                            <button class="btn saved" data-dismiss="modal">No</button>
                                        </div>

                                    </div>

                                            </div>
                                          </div>
 
                                    </div>
                                    <%}%>
                                     <%}else{ %>
                                    <tr>
                                        <td colspan="3">No user</td>
                                    </tr>
                                <% } %>
                                </tbody>
                            </table>
