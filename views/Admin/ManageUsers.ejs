 <script src="/lib/angular.min.js"></script>
    <script>
  
        
    
	  function discard(clicked_id){
			   
			     var data = {};
					data.id = clicked_id;
					console.log(data.id);
			    //alert(data.pan);
			   
				
				   
					$.ajax({
						type: 'POST',
						data: JSON.stringify(data),
				        contentType: 'application/json',
                        url: '/admin/Managedelete',						
                        success: function(data)
                        {       
                            window.location = data.redirect   
                        }
			   
		          })
					 
					
			   
		   }  
 </script>
    
