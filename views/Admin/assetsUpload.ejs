                <!-- /.row -->

            
                    <form action="assetsValueUpdate" method="post">
                        <div class="row">
                    <div class="col-md-6">
                       
                     <table class="table table-bordered table-hover">
                                <thead>
                        
                                    <tr>
                                        <th>Sl.No</th>
                                        <th>Risk Profile</th>
                                        <th>Category</th>
                                        <th>Value</th>
                                    </tr>
                                </thead>
                                <tbody>
                                        
                                       
                                
                                     <%for (var i=0;i<assetsData.length;i++){%>  
                                        <tr>
                                        <td id="slno" name="slno" ng-model="orderid"><%=i+1%></td>
                                        <td id="riskprofile" name="riskprofile" ><%= assetsData[i].riskprofile%></td>
                                        <td id="category" name="category"><%=assetsData[i].category%></td>
                                        <td id="value" name="value" contenteditable='true' ><%=assetsData[i].value%></td>
                                          </tr>
                                        <%}%>
                                 
                                
                                     
                                    
                                   
                                </tbody>
                            </table>
                            
                             </div>
                         <div class="col-md-6">
                             <div class="form-group">
                             Select Risk Profile :
                            
                             <select id="riskprofileval"  name="riskprofileval" style="margin-left:  3.4em;height: 2em;
    font-weight: 600;">
                                                    <option value="Conservative">Conservative </option>
                                                    <option value="Moderate">Moderate </option>
                                                    <option value="Aggressive">Aggressive </option>

                                                </select>
                                  
                             <br><br>
                             </div>
                             Select Risk category :
                             
                             <select id="categoryval"  name="categoryval" style="margin-left: 2.5em;height: 2em;font-weight: 600;">
                                                    <option value="Equity">Equity </option>
                                                    <option value="Hybrid">Hybrid </option>
                                                    <option value="Debt">Debt </option>

                                                </select>
                             <br><br>
                               
                             Enter value to be updated :
                                
                             <input type="text" id="valueval" name="valueval" style="width:8em;height: 2em;font-weight: 600;">
                             
                              
                            <br> <br>
                             <input type="submit">
                             <% if(message.length>0){ %>
                    <div class="alert alert-danger" style="margin-top:10px;width:350px;"><%=message %></div>
                        <%}%> 
                            </div> 
                            
                             
                            </div>
                        
                        </form>
                    
                
        
           
