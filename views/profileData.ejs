   <script>
    var calculateAge = function() {
        var today = new Date();
        var birthDate = new Date($('#dob').val());
        var age = today.getFullYear() - birthDate.getFullYear();
        var m = today.getMonth() - birthDate.getMonth();
        if (m < 0 || (m === 0 && today.getDate() < birthDate.getDate())) {
            age--;
        }
        return age;
    };

    var validatePinCode = function() {
        var pincode = $('#pincode').val();
        if (pincode.length != 6) {
            $('#errMsgPincode').slideDown();
            return false;
        } else {
            $('#errMsgPincode').slideUp();
            return true;
        }
    };

    var validatePan  = function() {
        var pan = $('#pan').val();
        var regExp = /[a-zA-z]{5}\d{4}[a-zA-Z]{1}/;
        if (pan.length == 10 && pan.match(regExp) ) {
            var pan = $('#errMsgPan').slideUp();
            return true;
        } else {
            var pan = $('#errMsgPan').slideDown();
            return false;
        }
    };

    var checkDOB = function() {
        if ( calculateAge() < 18 || isNaN(calculateAge()) ) {
            $('#dob').val('');
            $('#dob').focus();
            $('#dateValidationMsg').slideDown();
            return false;
        } else {
            $('#dateValidationMsg').slideUp();
            return true;
        }
    };

    var validateFrom = function() {
        var dobOk = checkDOB();
        var pinOk = validatePinCode();
        var panOK = validatePan();
        if (dobOk && pinOk && panOK)
            return true;
        else
            return false;
    };
   
       
