                            </div>                                   
                            
                            <div class="form-group">
                                <div class="dropdown">
                                    <label class="control-label" for="text">Gender:</label>
                  				   <% var Gender = [
    "Male", "Female"
    ];%>                                                                      
                                    <select class="form-controler edit selectGender" id="gender" name="gender"  ><span class="caretNext caretNext1 caret "></span>
					<% Gender.forEach( function( sex ){ %>
					
					                    <option valuw="<%= sex%>" <% if(profile.gender == sex) {%> selected="selected"<%}%> >
						 <%= sex%>
					                    </option>
					 <% });%>
                                    </select>                               
                                </div>        
                            </div>
                            <div class="form-group">
                                <label class="control-label" >Address:</label>
                                <textarea  class="form-controler addressSection2" rows="6" id="comment" type="text"  name="address"  maxlength="200" required/><%= profile.address %></textarea>
                            
                            </div>
                        </div>
                    </div>
                                                      
                      <div id="ProfileColumn2"  class="col-md-6 rightSetting">
                          
                          <div style="width: 280px; float: left">                                  
