<!DOCTYPE html>
<html lang="en">
<head>

 <% include partials/head %>
    </head>
<body>
  <% include partials/header %> 
      <div class="wrapper" style="overflow-y:scroll;overflow-x:hidden;">
	  <div class="main">
<div class="container">
<div class="savedPlansPage">  
      <div class="headGoal">
          <h3 class="savedPlans">Saved Plans</h3>
             <em>All plans are valid upto 30 days from the date paid</em> 
            </div>
            
	<% if(!plansHeader) {%>
	<h1>You have not selected any goal yet</h1>
	<% } %>
	
       <% for(j=0;j<plansHeader.length;j++) {%>

            <div class="row" id="savedPlans">
                <div class="col-md-4">
                        <div class="myGoal">
                                <!--<img src="images/goals/Rich/Crorepati.png" mood="Rich" alt="Home view" >-->
                                <h4> My Goal <%= plansHeader.length %></h4>
                        </div>
                </div>
                
                 <div class="col-md-4">
					  <label>Goal Name:</label><p><%=plansHeader[j].name %></p><br/>
                                
                                 <label>Risk Profile:</label><p><%=plansHeader[j].riskprofile %></p><br/>
                                 <label>Amount to invest:</label><p><%=plansHeader[j].sip %></p>
                
                </div>
                
                
                <div class="col-md-4">
                                <label>No. of years:</label><p><%=plansHeader[j].totalyears %></p><br/>
                               <label>Goal Outcome:</label><p><%=plansHeader[j].masteramount %></p><br/>
                               
                </div>
                
                </div>
            
	
	  <div class="modal-body">
            <div class="pageSavedPlan">
                                 
                       <div class="table-responsive">
                        <table class="table">
    <thead>
      <tr>
        <th>Sl No.</th>
        <th>Category</th>
        <th>Percentage</th>
        <th>Amount (&#x20B9;)</th>
      </tr>
    </thead>
    <tbody>
		 <% for(k=0,l=1;k<assetPlanDetail.length;k++,l++) {%>
	 <tr>
		 <td><%= l %></td>
	 <td><%=assetPlanDetail[k].allocationcategory %></td>
		 <td><%=assetPlanDetail[k].allocationpercentage %></td>
	  <td><%=assetPlanDetail[k].allocationamount %></td>
	 </tr>

<%}%>
         
  
    </tbody>
                      
                           
  </table>
                                 
					   
                                   </div>                
                
        </div>
       
        </div>
	
	
          	<% if(plansDetail) {%>
	
     <div class="modal-body">
            <div class="pageSavedPlan">
                                 
                       <div class="table-responsive">
                        <table class="table">
    <thead>
      <tr>
        <th>Sl No.</th>
        <th>Description</th>
        <th>Category</th>
        <th>Amount (&#x20B9;)</th>
      </tr>
    </thead>
    <tbody>
		
		 <% for(i=0,m=1;i<plansDetail.length;i++,m++) {%>
	 <tr>
		 <td><%= plansDetail[2] %></td>

	 </tr>

<%}%>
         
  
    </tbody>
                      
                           
  </table>
                          
 <div class="pull-right" style="margin-left:10%;margin-right:10%;">
                          
	 <form action="/Proceed" method="post">	
		 <input id="savedplanid" name="savedplanid" value="<%=plansHeader[j].savedplanid %>" hidden="hidden">
	<button class="btn saved" type="submit">Proceed</button>
				</form>	
	 
                        
                     <button class="btn saved" id="<%=plansHeader[j].savedplanid %>" onClick="discard(this.id)">Discard</button>
                  
                        </div>          
					   
                                   </div>                
                
        </div>
       
        </div>
	<% } %>
       <%}%>
      </div>
        <% include partials/footer %>   
    </div>
    </div>
    </div>

  <div class="container">
 


  <div class="modal fade" id="myModalDiscard" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        
        <div class="modal-body">
          <p>Are you sure you want to DISCARD?</p>
            <button class="btn saved" >YES</button>
            <button class="btn saved">No</button>
        </div>
       
      </div>
      
    </div>
  </div>
  
</div>
	
	<script>
		
		
		
	
	  function discard(clicked_id){
			   
			     var data = {};
					data.savedplanid = clicked_id;
					console.log(data.savedplanid);
			    //alert(data.pan);
			   
				 <% if(loggedIn) {%>
				   
					$.ajax({
						type: 'POST',
						data: JSON.stringify(data),
				        contentType: 'application/json',
                        url: '/Discard',						
                        success: function(data) {
                            console.log('success');
							     //bseDisp =true;
	
                           // console.log(JSON.stringify(data));
                        }
			   
		   })
					 <% }%>
					
			   
		   }
		   
		   
	
	
	</script>
	
	
      <script src="javascripts/TweenMax.min.js"></script>
<script src="javascripts/TextPlugin.min.js"></script>
         <script src="javascripts/main.js"></script>
    <script src="javascripts/control.js" type="text/javascript"></script>                   
</body>
</html>







